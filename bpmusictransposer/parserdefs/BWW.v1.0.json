{
	"_docstring": {
		"TakenFrom": "http://bagpipe.ddg-hansa.com/Bagpipe_Reader.pdf",
		"FormatName": "BagpipeMusicWriter",
		"FormatVersion": "v1.0",
		"FormatExtensions": ["bww"],
    "Encoding": "cp1252",
		"__fontspec__": "(?:<align>[LCRA]),(?:<x>\\d+),(?:<y>\\d+),(?:<font>[^,]*),(?:<size>\\d+),(?:\\d+,){6}(?:\\d+)",
		"HeaderInfo": [
      ["\"Converted from  BMW Dos file format to Bagpipe Reader 1.0 format using BMWFC32\\.\""],
      ["Bagpipe Music Writer Gold:1.0"],
      ["\\S*Bagpipe Reader:1.0"],
			["TuneTempo,(?P<tempo>\\d+)", "tempo"],
      ["(Bagpipe )?Bagpipe Musicworks Gold:1.0"],
      ["MIDINoteMappings,\\((\\d+\\s*,\\s*){16,}\\d+\\)"],
      ["FrequencyMappings,\\((\\d+\\s*,\\s*){16,}\\d+\\)"],
      ["InstrumentMappings,\\((\\d+\\s*,\\s*){4,}\\d+\\)"],
      ["GracenoteDurations,\\((\\d+\\s*,\\s*){8,}\\d+\\)"],
      ["FontSizes,\\((\\d+\\s*,\\s*)+\\d+\\)"],
      ["TuneFormat,\\((\\d+,){2}[FM],[LRC],(\\d+\\s*,\\s*){4}[LP]\\s*,\\s*\\d+\\s*,\\s*\\d+\\)?"],
			["\"(?P<title>[^\"]*)\",\\([RT],{{_fontspec}}", "title"],
			["\"(?P<tunetype>[^\"]*)\",\\([IY],{{_fontspec}}", "tunetype"],
			["\"(?P<composer>[^\"]*)\",\\(M,{{_fontspec}}", "composer"],
			["\"(?P<footer>[^\"]*)\",\\([FX],{{_fontspec}}", "footer"],
			["\"(?P<footer>[^\"]*)\"", "footer"],
      ["^(?:\\d+,)+\\d+\\)"],
      [",\\([RTIYMFX]?,{{_fontspec}}"]
    ],
    "Annotations": {
    }
	},
	"_": {
		"_fontspec": "(?:[LCRAlcra])?\\s*,\\s*(?:\\d+\\s*,\\s*){2}(?:[^,]*)\\s*,\\s*(?:\\d+)\\s*,\\s*(?:\\d+\\s*,\\s*){6,}(?:\\d+)[\\)}\\]]",
    "note": "{{note}}[lr]?_{{time}}",
		"dots": "'+",
    "state": "[se]",
		"time": "\\d+",
    "count": "\\d"
	},
  "_modifiers": {
    "half": { "_pattern": "h{{item}}",
      "_modify": {"half": true},
      "_applies": ["double", "grip", "birl", "darado", "pele", "throw", "strike", "dblstrike", "trplstrike"]
	  },
    "thumb": { "_pattern": "t{{item}}",
			"_modify": {"thumb": true},
			"_applies": ["double", "birl", "strike", "grip"]
    },
    "heavy": { "_pattern": "g{{item}}",
      "_modify": {"heavy": true},
      "_applies": ["birl", "strike", "grip"]
    },
    "a": { "_pattern": "a{{item}}",
      "_modify": {"A": true},
      "_applies": ["birl"]
    },
    "low": { "_pattern": "l{{item}}",
      "_modify": {"low": true},
      "_applies": ["pele", "gracestrike"]
    },
    "low_half": { "_pattern": "lh{{item}}",
      "_modify": {"low": true},
      "_applies": ["strike"]
    },
    "state": { "_pattern": "{{item}}(?P<state>[se])?",
      "_modify": {"state": {"s": "start", "e": "end"}},
      "_applies": ["nlets", "tie"]
    }
  },
  "_postprocess": {
    "C": "common_time"
  },
	"note": {
		"LG": "LG",
		"LA": "LA",
		"B": "B",
		"C": "C",
		"D": "D",
		"E": "E",
		"F": "F",
		"HG": "HG",
		"HA": "HA"
	},
	"snote": {
		"LG": "lg",
		"LA": ["la", "a"],
		"B": "b",
		"C": "c",
		"D": "d",
		"E": "e",
		"F": "f",
		"HG": "hg",
		"HA": "ha"
	},
	"gnote": {
		"LG": "lg",
		"LA": "la",
		"B": "b",
		"C": "c",
		"D": "d",
		"E": "e",
		"F": "f",
		"HG": "g",
		"HA": ["t", "a"]
	},
  "rest": "REST_{{time}}",
  "gracestrike": "{{gnote}}st{{snote}}",
  "doublegrace": "{{gnote}}{{snote}}",
  "time_notation": "{{time:0}}_{{time:1}}",
  "cut_common_time": "C_",
	"dot": "(?P<dot>'+){{snote}}",
	"clefc": "&",
	"grace": "{{gnote}}g",
	"double": "db{{snote}}",
	"strike": "str?{{snote}}",
	"grip": "grp{{snote}}?",
	"tarluath": "tar",
	"dtarluath": "tarb",
	"darado": ["bubly", "darado"],
	"birl": ["brl", "br"],
	"throw": "thrd",
	"dblstrike": "st2{{snote}}",
	"trplstrike": "st3{{snote}}",
	"tie": ["\\^t", "\\^t{{snote}}"],
  "nlets": "\\^{{count}}{{time}}?{{snote}}?",
	"cadence": "cad{{snote}}",
	"rodin": "rodin",
  "pele": "pel{{snote}}",
  "edre": "edre",
	"repeatstart": "I!''",
	"repeatend": "''!I",
	"barend": "!",
	"lineend": "!t",
	"partstart": "I!",
	"partend": ["!I", "''!It", "!!t"],
  "endingstart": "'{{time}}",
  "endingend": "_'",
  "fermata": "fermat{{snote}}",
  "sharp": "sharp{{snote}}",
  "natural": "natural{{snote}}",
  "flat": "flat{{snote}}",
  "_ignore": "space"
}

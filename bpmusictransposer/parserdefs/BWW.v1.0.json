{
	"_docstring": {
		"TakenFrom": "http://bagpipe.ddg-hansa.com/Bagpipe_Reader.pdf",
		"FormatName": "BagpipeMusicWriter",
		"FormatVersion": "v1.0",
		"FormatExtensions": ["bww"],
		"__fontspec__": "(?:<align>[LCRA]),(?:<x>\\d+),(?:<y>\\d+),(?:<font>[^,]*),(?:<size>\\d+),(?:\\d+,){6}(?:\\d+)",
		"HeaderInfo": [
      ["\"Converted from  BMW Dos file format to Bagpipe Reader 1.0 format using BMWFC32\\.\""],
			["\"(?P<title>[^\"]*)\",\\(T,{{_fontspec}}\\)", "title"],
			["\"(?P<tunetype>[^\"]*)\",\\([IY],{{_fontspec}}\\)", "tunetype"],
			["\"(?P<composer>[^\"]*)\",\\(M,{{_fontspec}}\\)", "composer"],
			["TuneTempo,(?P<tempo>\\d+)", "tempo"],
      ["Bagpipe Reader:1.0"],
      ["Bagpipe Music Writer Gold:1.0"],
      ["MIDINoteMappings,\\((\\d+\\s*,\\s*){20,26}\\d+\\)"],
      ["FrequencyMappings,\\((\\d+,){20,26}\\d+\\)"],
      ["InstrumentMappings,\\((\\d+,){4,6}\\d+\\)"],
      ["GracenoteDurations,\\((\\d+,){8,12}\\d+\\)"],
      ["FontSizes,\\((\\d+,)+\\d+\\)"],
      ["TuneFormat,\\((\\d+,){2}[FM],[LC],(\\d+,){4}[LP],\\d+,\\d+\\)"],
			["\"(?P<footer>[^\"]*)\",\\([FX],{{_fontspec}}\\)", "footer"],
			["\"(?P<footer>[^\"]*)\"", "footer"]
    ],
    "Annotations": {
    }
	},
	"_": {
		"_fontspec": "(?:[ITYMF],)?(?:[LCRA]),(?:\\d+),(?:\\d+),(?:[^,]*),(?:\\d+),(?:\\d+,){6,7}(?:\\d+)",
    "note": "{{note}}[lr]?_{{time}}",
		"dots": "'+",
    "state": "[se]",
		"time": "\\d+",
    "count": "\\d"
	},
  "_modifiers": {
    "half": { "_pattern": "h{{item}}",
      "_modify": {"half": true},
      "_applies": ["double", "grip", "birl", "darado", "pele", "throw", "dblstrike", "trplstrike"]
	  },
    "thumb": { "_pattern": "t{{item}}",
			"_modify": {"thumb": true},
			"_applies": ["double", "birl", "strike"]
    },
    "heavy": { "_pattern": "g{{item}}",
      "_modify": {"heavy": true},
      "_applies": ["birl", "strike"]
    },
    "a": { "_pattern": "a{{item}}",
      "_modify": {"A": true},
      "_applies": ["birl"]
    },
    "state": { "_pattern": "{{item}}(?P<state>[se])?",
      "_modify": {"state": {"s": "start", "e": "end"}},
      "_applies": ["nlets", "tie"]
    }
  },
  "_postprocess": {
    "C": "common_time"
  },
	"note": {
		"LG": "LG",
		"LA": "LA",
		"B": "B",
		"C": "C",
		"D": "D",
		"E": "E",
		"F": "F",
		"HG": "HG",
		"HA": "HA"
	},
	"snote": {
		"LG": "lg",
		"LA": ["la", "a"],
		"B": "b",
		"C": "c",
		"D": "d",
		"E": "e",
		"F": "f",
		"HG": "hg",
		"HA": "ha"
	},
	"gnote": {
		"LG": "lg",
		"LA": "la",
		"B": "b",
		"C": "c",
		"D": "d",
		"E": "e",
		"F": "f",
		"HG": "g",
		"HA": "t"
	},
  "rest": "REST_{{time}}",
  "gracestrike": "{{gnote}}st{{snote}}",
  "doublegrace": "{{gnote}}{{snote}}",
  "time_notation": "{{time:0}}_{{time:1}}",
  "cut_common_time": "C_",
	"dot": "(?P<dot>'+){{snote}}",
	"clefc": "&",
	"grace": "{{gnote}}g",
	"double": "db{{snote}}",
	"strike": "str?{{snote}}",
	"grip": "grp",
	"dgrip": "grpb",
	"tarluath": "tar",
	"dtarluath": "tarb",
	"darado": ["bubly", "darado"],
	"birl": ["brl", "br"],
	"throw": "thrd",
	"dblstrike": "st2{{snote}}",
	"trplstrike": "st3{{snote}}",
	"tie": ["\\^t", "\\^t{{snote}}"],
  "nlets": "\\^{{count}}{{time}}?{{snote}}?",
	"cadence": "cad{{snote}}",
	"rodin": "rodin",
  "pele": "pel{{snote}}",
	"repeatstart": "I!''",
	"repeatend": "''!I",
	"barend": "!",
	"lineend": "!t",
	"partstart": "I!",
	"partend": "!I",
  "endingstart": "'{{time}}",
  "endingend": "_'",
  "_ignore": "space"
}

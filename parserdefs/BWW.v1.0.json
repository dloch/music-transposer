{
	"_docstring": {
		"TakenFrom": "http://bagpipe.ddg-hansa.com/Bagpipe_Reader.pdf",
		"FormatName": "BagpipeMusicWriter",
		"FormatVersion": "v1.0",
		"FormatExtensions": ["bww"],
		"_fontspec": "(?:[ITYMF],)?(?:[LCRA]),(?:\\d+),(?:\\d+),(?:[^,]*),(?:\\d+),(?:\\d+,){6}(?:\\d+)",
		"__fontspec__": "(?:<align>[LCRA]),(?:<x>\\d+),(?:<y>\\d+),(?:<font>[^,]*),(?:<size>\\d+),(?:\\d+,){6}(?:\\d+)",
		"HeaderInfo": ["Bagpipe Reader:1.0",
			"MIDINoteMappings,\\((\\d+,){20,26}\\d+\\)",
			"FrequencyMappings,\\((\\d+,){20,26}\\d+\\)",
			"InstrumentMappings,\\((\\d+,){4,6}\\d+\\)",
			"GracenoteDurations,\\((\\d+,){8,12}\\d+\\)",
			"FontSizes,\\((\\d+,)+\\d+\\)",
			"TuneFormat,\\((\\d+,){2}[FM],L,(\\d+,){4}P,\\d+,\\d+\\)",
			"\"Converted from  BMW Dos file format to Bagpipe Reader 1.0 format using BMWFC32\\.\"",
			"\"(?P<title>[^\"]*)\",\\(T,{{_fontspec}}\\)",
			"\"(?P<tunetype>[^\"]*)\",\\(Y,{{_fontspec}}\\)",
			"\"(?P<composer>[^\"]*)\",\\(M,{{_fontspec}}\\)",
			"\"(?P<footer>[^\"]*)\",\\(F,{{_fontspec}}\\)",
			"TuneTempo,(?P<tempo>\\d+)"
		]
	},
	"_": {
		"note": "{{note}}[lr]?_{{time}}",
		"rest": "REST_{{time}}",
		"dots": "'+",
		"half{{any}}": "h{{any}}",
		"time": "\\d+",
		"gracestrike": "{{snote:0}}st{{snote:1}}",
		"time_notation": "{{time:0}}_{{time:1}}"
	},
	"note": {
		"LG": "LG",
		"A": "LA",
		"B": "B",
		"C": "C",
		"D": "D",
		"E": "E",
		"F": "F",
		"G": "HG",
		"HA": "HA"
	},
	"snote": {
		"LG": "lg",
		"A": "la",
		"B": "b",
		"C": "c",
		"D": "d",
		"E": "e",
		"F": "f",
		"G": "hg",
		"HA": "ha"
	},
	"gnote": {
		"LG": "lg",
		"A": "la",
		"B": "b",
		"C": "c",
		"D": "d",
		"E": "e",
		"F": "f",
		"G": "g",
		"HA": "t"
	},
	"dot": "'{{snote}}",
	"clefc": "&",
	"grace": "{{gnote}}g",
	"double": "db{{snote}}",
	"strike": "str{{snote}}",
	"grip": "grp",
	"hgrip": "hgrp",
	"dgrip": "grpb",
	"tarluath": "tar",
	"dtarluath": "tarb",
	"darado": ["bubly", "darado"],
	"birl": "brl",
	"heavybirl": "gbr",
	"throw": "thrd",
	"dblstrike": "st2{{snote}}",
	"trplstrike": "st3{{snote}}",
	"tie": "^t",
	"nlets": {
		"simple": ["^{{count}}s", "^{{count}}e"],
		"general": ["^{{count}}{{time}}s", "^{{count}}{{time}}e"]
	},
	"cadence": "cad{{snote}}",
	"rodin": "rodin",
